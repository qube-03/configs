# GIMP gimprc
# 
# This is your personal gimprc file.  Any variable defined in this file takes
# precedence over the value defined in the system-wide gimprc:
# /etc/gimp/2.0/gimprc
# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(interpolation-type linear)
(default-image
    (width 32)
    (height 64)
    (unit pixels)
    (xresolution 300)
    (yresolution 300)
    (resolution-unit inches)
    (image-type rgb)
    (precision u8-gamma)
    (color-managed yes)
    (color-profile NULL)
    (fill-type transparent)
    (comment "made^2"))
(default-grid
    (style solid)
    (fgcolor (color-rgba 0 0 0 1))
    (bgcolor (color-rgba 1 1 1 1))
    (xspacing 118.11)
    (yspacing 118.11)
    (spacing-unit centimeters)
    (xoffset 0)
    (yoffset 0)
    (offset-unit centimeters))
(undo-size 3G)
(import-raw-plug-in "${gimp_plug_in_dir}/plug-ins/file-raw-placeholder/file-raw-placeholder")
(export-metadata-exif no)
(export-metadata-xmp no)
(export-metadata-iptc no)
(debug-policy never)
(cursor-mode crosshair)
(monitor-xresolution 92)
(monitor-yresolution 92)
(default-view
    (show-menubar yes)
    (show-statusbar yes)
    (show-rulers no)
    (show-scrollbars no)
    (show-selection yes)
    (show-layer-boundary yes)
    (show-canvas-boundary yes)
    (show-guides yes)
    (show-grid no)
    (show-sample-points yes)
    (snap-to-guides yes)
    (snap-to-grid no)
    (snap-to-canvas no)
    (snap-to-path no)
    (padding-mode default)
    (padding-color (color-rgb 1 1 1))
    (padding-in-show-all no))
(toolbox-color-area no)
(toolbox-wilber no)
(theme "System")
(icon-theme "Symbolic-High-Contrast")
(action-history-size 256)
(color-profile-policy convert)
(layer-new-name "\\")
(selection-shrink-radius 8)
(selection-border-radius 8)
(fill-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10))
(stroke-options
    (style solid)
    (antialias no)
    (feather no)
    (feather-radius 10)
    (method paint-method)
    (width 1)
    (unit pixels)
    (cap-style butt)
    (join-style miter)
    (miter-limit 0)
    (dash-offset 0)
    (dash-info 0)
    (emulate-brush-dynamics no))


# end of gimprc
